ck=\"toBook(item[0].row, place.seat)\"\n                        >\n                            <div :style=\"place.is_free ? {'background': 'green'} : { 'background': 'red' }\">\n                                {{ place.seat }}\n                            </div>\n                        </div>\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </modal>\n</template>\n\n<script>\nimport modal from '../modals/modal.vue';\nimport { getAPI } from '@/API.js';\nconst movieList = new getAPI();\nexport default {\n    components: {\n        modal\n    },\n    props: ['placeData'],\n    methods: {\n        toBook(row, place) {\n            const obj = {\n                'movie_id': this.placeData.id,\n                'row': row,\n                'seat': place,\n                'showdate': this.placeData.date,\n                'daytime': this.placeData.time\n            }\n            movieList.toBook(obj).then(resp => {\n                console.log('resp', resp)\n                this.$emit('showTicket', resp.data.data);\n            })\n            .catch(err => console.error(err));\n\n\n        }\n    }\n}\n</script>\n\n<style scoped>\n    .place-container-div {\n        padding: 5px; \n        cursor: pointer; \n        width: 50px; \n        text-align: center;\n        transition: 0.5s;\n    }\n    .place-container-div:hover {\n        background: red;\n    }\n</style>\n"]}]}